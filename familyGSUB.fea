@IPA_LC_LATIN = [ alphalatin betalatin gammalatin iotalatin upsilonlatin philatin chilatin ];

	cvParameters {
		FeatUILabelNameID {
			name 3 1 0x0409 "Latinized Greek letters [ \0251 \A7B5 \0263 \0269 \028A \0278 \AB53 ] (IPA)"; # English US
			name 3 1 0x0809 "Latinized Greek letters [ \0251 \A7B5 \0263 \0269 \028A \0278 \AB53 ] (IPA)"; # English GB
			name 1 0 0 "Latinized Greek letters (IPA)"; # Mac English
			# name 3 1 0x0408 "\03A3\03AC\03BC\03B7 \014A"; # Greek FIX THIS!
			# name 3 1 0x0419 "\0441\0430\0430\043C\0441\043A\0438\0439 \014A"; # Russian FIX THIS!
		};
	};
	
	lookup GREEK_TO_IPA { # Latin script forms (substitutes Greek characters)
		sub @IPA_LC_GREEK by @IPA_LC_LATIN;
	} GREEK_TO_IPA;

} cv98;

